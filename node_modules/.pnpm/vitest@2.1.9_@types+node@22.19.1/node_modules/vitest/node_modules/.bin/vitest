#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/LegoAIBOS/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.1/node_modules/vitest/node_modules:/workspaces/LegoAIBOS/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.1/node_modules:/workspaces/LegoAIBOS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/LegoAIBOS/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.1/node_modules/vitest/node_modules:/workspaces/LegoAIBOS/node_modules/.pnpm/vitest@2.1.9_@types+node@22.19.1/node_modules:/workspaces/LegoAIBOS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
