{
  "name": "aibos",
  "private": true,
  "packageManager": "pnpm@9.12.1",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tests/*"
  ],
  "scripts": {
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "test:all": "pnpm -r test && pnpm --filter @aibos/tests test",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/react": "^18.3.5",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@remix-run/router": "1.19.2",
    "cookie": "^1.0.2",
    "react-router": "6.26.2",
    "scheduler": "^0.27.0",
    "set-cookie-parser": "^2.7.2"
  }
}
